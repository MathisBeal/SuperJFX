<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER" prefHeight="600.0" prefWidth="900.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.xen.superjfx.JukeboxController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="600.0" prefWidth="110.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Options">
               <VBox.margin>
                  <Insets bottom="50.0" top="20.0" />
               </VBox.margin>
            </Text>
            <Button mnemonicParsing="false" onAction="#actionMenu" prefWidth="96.0" text="Menu principal">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#actionMenu" prefWidth="96.0" text="Balles">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#actionMenu" prefWidth="96.0" text="3D">
               <VBox.margin>
                  <Insets bottom="15.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="230.0">
         <children>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerCR" prefHeight="50.0" prefWidth="175.0" text="Crab Rave">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerSSBU" prefHeight="50.0" prefWidth="175.0" text="Super Smash Bros Ultimate">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerBOTW" prefHeight="50.0" prefWidth="175.0" text="The Legend of Zelda">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerTLOU" prefHeight="50.0" prefWidth="175.0" text="The Last Of Us">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
      <VBox alignment="CENTER" prefHeight="600.0">
         <children>
            <StackPane fx:id="disque_global">
               <children>
                  <Circle fx:id="disque" fill="DODGERBLUE" radius="150.0" stroke="BLACK" strokeType="INSIDE" styleClass="disque_vinyl" />
                  <Circle fill="#222222" radius="40.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" styleClass="vinyl_centre" />
               </children>
               <padding>
                  <Insets left="15.0" right="15.0" />
               </padding>
            </StackPane>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#Lecture" prefHeight="30.0" prefWidth="75.0" text="Lecture">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#Pause" prefHeight="30.0" prefWidth="75.0" text="Pause">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#Stop" prefHeight="30.0" prefWidth="75.0" text="Stop">
                     <HBox.margin>
                        <Insets left="5.0" right="5.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
         <HBox.margin>
            <Insets left="15.0" right="15.0" />
         </HBox.margin>
      </VBox>
      <VBox alignment="CENTER" prefHeight="600.0" prefWidth="230.0">
         <children>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerVII" prefHeight="50.0" prefWidth="175.0" text="Final Fantsay VII Remake">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerMK" prefHeight="50.0" prefWidth="175.0" text="Mortal Kombat">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerMII" prefHeight="50.0" prefWidth="175.0" text="ChaÃ®ne Mii">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
            <Button id="jukebox_boutons" mnemonicParsing="false" onAction="#LancerMario" prefHeight="50.0" prefWidth="175.0" text="New Super Mario Bros Wii">
               <VBox.margin>
                  <Insets bottom="15.0" top="15.0" />
               </VBox.margin></Button>
         </children>
      </VBox>
   </children>
</HBox>
